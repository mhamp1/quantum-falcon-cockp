{
  "name": "@quantumfalcon/license-hub",
  "version": "2025.1.0",
  "description": "Centralized license authority and validation for Quantum Falcon ecosystem",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts --clean",
