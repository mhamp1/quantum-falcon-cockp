  },
  "devDependencies": {
    "@types/crypto-js": "^4.2.2",
    "@types/node": "^20.10.0",
    "eslint": "^9.0.0",
    "jest": "^29.7.0",
    "tsup": "^8.0.1",
    "typescript": "^5.7.2"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com",
    "@quantumfalcon:registry": "https://npm.pkg.github.com"
