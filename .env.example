# Quantum Falcon Cockpit - Environment Configuration
# Copy this file to .env and fill in your values
# NEVER commit .env to version control!

# License Verification API
# This should point to your secure backend server
# that has access to your master key and license database
VITE_LICENSE_API_ENDPOINT=https://your-secure-api.com/api/verify

# LicenseAuthority Purchase URL
VITE_PURCHASE_URL=https://github.com/mhamp1/LicenseAuthority

# API Endpoints (Optional - for production)
VITE_TRADING_API_ENDPOINT=https://your-api.com/api/trading
VITE_MARKET_DATA_ENDPOINT=https://your-api.com/api/market

# Security Settings
VITE_ENABLE_2FA=true
VITE_SESSION_TIMEOUT=300000

# Note: NEVER put your master key here or anywhere in this repository!
# The master key should ONLY exist on your secure backend server.
